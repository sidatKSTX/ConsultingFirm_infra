{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowManageOwnAccessKeys",
            "Effect": "Allow",
            "Action": [
                "iam:CreateAccessKey",
                "iam:DeleteAccessKey",
                "iam:GetAccessKeyLastUsed",
                "iam:GetAccountSummary",
                "iam:ListAccessKeys",
                "iam:UpdateAccessKey",
                "iam:ListAccountAliases"
                "iam:GetAccountPasswordPolicy"
                "iam:ListSTSRegionalEndpointsStatus"
            ],
            "Resource": [
                "arn:aws:iam::818140567777:user/${aws:username}"
            ]
        }
    ]
}
